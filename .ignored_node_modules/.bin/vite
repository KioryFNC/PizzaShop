#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/bin/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/bin/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules:/mnt/c/Users/yurib/Desktop/Study/RocketSeat/React/pizzashop-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.2.5_@types+node@22.1_35135b8c22ce1e54b999f31b5e3f3acb/node_modules/vite/bin/vite.js" "$@"
fi
